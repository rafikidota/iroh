{
  "name": "@rafikidota/iroh",
  "version": "0.5.59",
  "description": "Sometimes, the best way to solve your own problems is to help someone else.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafikidota/iroh.git"
  },
  "author": "rafiki <71207415+rafikidota@users.noreply.github.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "postbuild": "yarn move:crud && yarn move:init",
    "move:crud": "mkdir -p dist/schematics/crud/files/ts && cp -ar ./src/schematics/crud/files/ts/* dist/schematics/crud/files/ts",
    "move:init": "mkdir -p dist/schematics/init/files/ts && cp -ar ./src/schematics/init/files/ts/* dist/schematics/init/files/ts",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.ts\"",
    "prepublishOnly": "rimraf dist && npm run lint && npm run build"
  },
  "dependencies": {
    "@angular-devkit/core": "15.2.1",
    "@angular-devkit/schematics": "15.2.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@swc/core": "^1.6.0",
    "@swc/helpers": "^0.5.11",
    "bcrypt": "^5.1.1",
    "chalk": "4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.7",
    "tsup": "^8.1.0",
    "typescript": "5.3.3",
    "typescript-eslint": "^7.14.1"
  },
  "schematics": "./collection.json",
  "keywords": [
    "backend",
    "nestjs",
    "typeorm",
    "generic",
    "schematics",
    "crud",
    "auth"
  ]
}
